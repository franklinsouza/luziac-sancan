      <!-- end col-7 -->
      <div class="col-lg-5 offset-lg-2">
        <ul class="custom-list">
          <li><i class="lni lni-checkmark"></i> <p>Terrenos a partir de <span class="primary-color">1000 m²</span></p></li>
          <li><i class="lni lni-checkmark"></i> <p>+1 <span class="primary-color">Vaga náutica</span></p></li>
          <li><i class="lni lni-checkmark"></i> <p>Menos de <span class="primary-color">80km</span> de São Paulo</p></li>
          <li><i class="lni lni-checkmark"></i> <p>Mais de <span class="primary-color">30</span> ítens de lazer</p></li>
        </ul>
      </div>
      <!-- end col-5 -->
      </div>
      <!-- end row --> 
    </div>
    <!-- end container --> 
  </section>
  <!-- end content-section -->

