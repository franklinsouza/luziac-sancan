---
import Layout from "../../../layouts/Layout.astro";
import SingleHeader from "../../../components/layout/SingleHeader.astro";
import Chamada from "../../../components/layout/Chamada.astro";
import Image from "astro/components/Image.astro";

import logo from '../../../assets/images/acqua-ibiuna/logo.png';
import bgSection from '../../../assets/images/acqua-ibiuna/jetski.webp';
import foto1 from '../../../assets/images/acqua-ibiuna/1-800x500.webp';
import foto2 from '../../../assets/images/acqua-ibiuna/2-800x500.webp';
import foto3 from '../../../assets/images/acqua-ibiuna/3-800x500.webp';
import foto4 from '../../../assets/images/acqua-ibiuna/4-800x500.webp';
import foto5 from '../../../assets/images/acqua-ibiuna/5-800x500.webp';
import foto6 from '../../../assets/images/acqua-ibiuna/6-800x500.webp';
import foto7 from '../../../assets/images/acqua-ibiuna/7-800x500.webp';
